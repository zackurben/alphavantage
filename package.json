{
  "name": "alphavantage",
  "version": "2.0.1",
  "description": "A simple interface to the Alpha Vantage API.",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "jest --setupFiles dotenv/config",
    "lint": "prettier --write \"./!(dist)**/**.js\"",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "rollup index.js --file dist/bundle.js --format umd --name alphavantage"
  },
  "keywords": [
    "alpha vantage",
    "vantage",
    "alpha",
    "alphavantage",
    "stocks",
    "stock market",
    "api"
  ],
  "author": "Zack Urben <zackurben@gmail.com> (http://zackurben.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zackurben/alphavantage"
  },
  "homepage": "https://github.com/zackurben/alphavantage",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "cross-fetch": "^3.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "coveralls": "^3.0.9",
    "delay": "^4.3.0",
    "dotenv": "^8.2.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rollup": "^1.31.1"
  },
  "jest": {
    "collectCoverage": true,
    "bail": false,
    "verbose": true,
    "testMatch": [
      "**/test/**.js"
    ],
    "testEnvironment": "node"
  }
}
