{
  "name": "alphavantage",
  "version": "2.3.0",
  "description": "A simple interface to the Alpha Vantage API.",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "jest",
    "lint": "prettier --write \"./!(dist)**/**.js\"",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "rollup index.js --file dist/bundle.js --format umd --name alphavantage"
  },
  "keywords": [
    "alpha vantage",
    "vantage",
    "alpha",
    "alphavantage",
    "stocks",
    "stock market",
    "api"
  ],
  "author": "Zack Urben <zackurben@gmail.com> (http://zackurben.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zackurben/alphavantage"
  },
  "homepage": "https://github.com/zackurben/alphavantage",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "cross-fetch": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "babel-jest": "^28.0.2",
    "coveralls": "^3.1.1",
    "delay": "^5.0.0",
    "dotenv": "^10.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rollup": "^2.58.0"
  },
  "jest": {
    "collectCoverage": true,
    "bail": false,
    "verbose": true,
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ]
  }
